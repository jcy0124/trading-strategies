<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jcy.tradingstrategies.dao.CalendarDataDao">


    <select id="selectLast14Day" resultType="com.jcy.tradingstrategies.entity.CalendarDataEntity">
        select * from calendar_data where id &lt; #{id} order by id desc limit 14
    </select>
</mapper>

